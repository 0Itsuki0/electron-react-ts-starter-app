{
    "compilerOptions": {
        "target": "esnext",
        "module": "nodenext",
        "moduleResolution": "nodenext",
        "outDir": "./dist/electron-app",
        "rootDir": "./src/electron-app",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": false
    },
    "include": [
        "src/electron-app"
    ],
    "exclude": [
        "node_modules"
    ],
}